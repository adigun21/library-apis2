spring.datasource.url=jdbc:mysql://localhost:3306/library_db?serverTimezone=UTC

spring.datasource.username=root
spring.datasource.password=oracle11008



##Hibernate Properties
# The SQL dialect makes Hibernate generate better SQL for the cosen database
spring.jpa.properties.hibernate.dialect = org.hibernate.dialect.MySQL8InnoDBDialect

#Hibernate ddl auto
spring.jpa.hibernate.ddl-auto=none
