sppring.datasource.url=jdbc:mysql://localhost:3306/library_db
spring.datasource.data-username=root
spring.datasource.data-password=oracle11008


##Hibernate Properties
# The SQL dialect makes Hibernate generate better SQL for the cosen database
spring.jpa.properties.hibernate.dialect = org.hibernate.dialectMySQL8InnoDBDialect

#Hibernate ddl auto
spring.jpa.hibernate.ddl-auto=none
