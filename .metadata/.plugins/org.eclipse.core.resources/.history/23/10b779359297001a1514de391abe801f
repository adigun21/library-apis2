package com.libraryapis2.Author;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.GenerationType;
import javax.persistence.Id;
import javax.persistence.SequenceGenerator;
import javax.persistence.Table;

@Entity
@Table(name = "AUTHOR")
public class AuthorEntity {
	
	@Column(name = "author_Id")
	@Id
	@GeneratedValue(strategy = GenerationType.SEQUENCE, generator = "authorId_generator")
	@SequenceGenerator(name = "authorId_generator", sequenceName = "author_sequence", allocationSize = 50)
	private int authorid;
	
	@Column(name = "Name")
	private String firstName;
	
	@Column(name = "Last")
	private String lastName;
	
	@Column(name = "Date_Of_Birth")
	private String dateOfBirth;
	
	@Column(name = "ENUM")
	private Enum<?> gender;

	public AuthorEntity() {

	}

	

	public AuthorEntity(int authorid, String firstName, String lastName, String dateOfBirth, Enum<?> gender) {
		
		this.authorid = authorid;
		this.firstName = firstName;
		this.lastName = lastName;
		this.dateOfBirth = dateOfBirth;
		this.gender = gender;
	}



	public int getPublisherid() {
		return publisherid;
	}

	
	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getEmailId() {
		return emailId;
	}

	public void setEmailId(String emailId) {
		this.emailId = emailId;
	}

	public String getPhoneNumber() {
		return phoneNumber;
	}

	public void setPhoneNumber(String phoneNumber) {
		this.phoneNumber = phoneNumber;
	}

	
	
	

	
	
	
}

